// get data from the embedded h2 database in BAM
function QueryResults(q){
        
        var result;
        try{
            var db = new Database("WSO2BAM_DATASOURCE");
            result = db.query(q);
        }
        catch(e){
            result=null;
            //print(e);
        }
        finally{
            db.close();
        }
        return result;
}
   
