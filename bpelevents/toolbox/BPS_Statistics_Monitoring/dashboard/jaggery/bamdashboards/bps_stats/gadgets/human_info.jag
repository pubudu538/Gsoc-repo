// return font colour for task status
function getFontColour(state)
{

  var colour="black";

  if(state=="RESERVED")
  {
    colour="blue";
  }
  else if(state=="COMPLETED")
  {
    colour="green";
  }
  else if(state=="FAILED")
  {
    colour="red";
  }
  else if(state=="SUSPENDED")
  {
    colour="orange";
  }
  else if(state=="IN_PROGRESS")
  {
    colour="purple";
  }
  else if(state=="READY")
  {
    colour="black";
  }

  return colour;

}

